{
  "name": "divider",
  "type": "module",
  "version": "1.0.0",
  "description": "To divide string or string[] with a given separator",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "build": "tsc --module ESNext --outDir dist/esm && tsc --module CommonJS --outDir dist/cjs --outExtension .cjs",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "divider",
    "divide",
    "split",
    "string",
    "array"
  ],
  "files": [
    "dist",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nyaomaru/divider.git"
  },
  "homepage": "https://github.com/nyaomaru/divider.git",
  "author": "nyaomaru",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "prettier": "^3.5.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
