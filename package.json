{
  "name": "@nyaomaru/divider",
  "type": "module",
  "version": "1.9.9",
  "description": "To divide string or string[] with a given separator",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "scripts": {
    "test": "jest",
    "test:performance": "jest --config jest.performance.config.cjs",
    "test:unit": "jest --testPathIgnorePatterns=performance",
    "lint": "eslint .",
    "build": "tsup",
    "prepack": "pnpm run build",
    "typedoc": "typedoc"
  },
  "keywords": [
    "divider",
    "divide",
    "split",
    "string",
    "array"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nyaomaru/divider.git"
  },
  "homepage": "https://divider-docs.vercel.app/",
  "author": "nyaomaru",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.15.12",
    "bun-types": "^1.2.21",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.4.0",
    "jest": "^30.0.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "tsx": "^4.19.4",
    "typedoc": "^0.28.5",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/nyaomaru/divider/issues"
  }
}